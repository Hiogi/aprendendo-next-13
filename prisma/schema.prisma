// learn more about it in the docs: https://pris.ly/d/prisma-schema

// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  password String
}

model Dados {
  idConta    Int    @id @default(autoincrement())
  saldoConta BigInt
  status     String
}


